<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<nifty xmlns="http://nifty-gui.sourceforge.net/nifty-1.3.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nifty-gui.sourceforge.net/nifty-1.3.xsd http://nifty-gui.sourceforge.net/nifty-1.3.xsd">
  <useStyles filename="nifty-default-styles.xml"/>
  <useControls filename="nifty-default-controls.xml"/>
  <screen id="screen1">
    <layer childLayout="absolute" id="layer0">
      <panel childLayout="absolute" height="100%" id="panel0" style="nifty-panel-simple" width="100%" x="0" y="0">
        
          <image filename="Interface/OptionImage/title.jpg" height="88px" id="Title" width="372px" x="215" y="16"/>
        
        <panel childLayout="absolute" height="414px" id="panel2" style="nifty-panel-simple" width="681px" x="55" y="129">
          
          <image filename="Interface/OptionImage/Music.jpg" height="61px" id="Music" width="193px" x="36" y="50"/>
          <image filename="Interface/OptionImage/SFX.jpg" height="61" id="SFX" width="193" x="36" y="140"/>
          <image filename="Interface/OptionImage/3D Audio.jpg" height="61" id="image4" width="193" x="36" y="230"/>
          
          <control id="checkbox0" name="Music" x="348" y="70">
              <interact onClick="Music()"></interact>
          </control>
          
          <control id="checkbox1" name="SFX" x="348" y="155">
              <interact onClick="SFX()"></interact>
          </control>
          <control id="checkbox2" name="3D audio" x="348" y="250">
              <interact onClick="Audio3D()"></interact>
          </control>
          
          
          <control font="aurulent-sans-16.fnt" height="36px" id="label4" name="label" text="label4" width="163px" x="442px" y="154"/>
          <control font="aurulent-sans-16.fnt" height="36px" id="label3" name="label" text="label3" width="163px" x="442" y="67"/>
          <control font="aurulent-sans-16.fnt" height="36px" id="label5" name="label" text="label5" width="163px" x="442px" y="245"/>
          <control childLayout="center" height="8%" id="Option" label="Option" name="button" width="30%" x="50%" y="400">
                 <interact onClick="option()"/>
          </control>
        </panel>
      </panel>
    </layer>
  </screen>
</nifty>
